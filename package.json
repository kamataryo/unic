{
  "name": "unic",
  "version": "0.0.0",
  "description": "unicode synthesizer web app",
  "main": "./dist/index.html",
  "scripts": {
    "clean": "rimraf dist",
    "start":
      "cross-env NODE_ENV=development DEBUG=true webpack-dev-server --progress --colors --inline --watch",
    "build": "cross-env NODE_ENV=production DEBUG=false webpack -p",
    "test": "NODE_ENV=test jest",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "lint": "npm run lint:style && npm run lint:es",
    "lint:style": "stylelint  './src/**/*.js' './src/**/*.jsx'",
    "lint:es": "eslint --cache --ext=jsx --ext=js .",
    "lint:prettier":
      "prettier-eslint \"./src/**/*.js\" \"./src/**/*.jsx\" \"./test/**/*.js\" \"./test/**/*.jsx\" --write",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "styled-components": "^2.2.0",
    "switz": "^0.0.20"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.11.6",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "jest": "^21.2.1",
    "json-loader": "^0.5.4",
    "mock-local-storage": "^1.0.2",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.4",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier --write", "git add"]
  },
  "keywords": ["unicode", "synthesize", "asciiart"]
}
